APP_ENV=development
APP_DOMAIN=localhost
APP_URL="http://localhost:3000"
APP_DEV_BUILD_URL="http://localhost:4000"

# Vercel
ENABLE_EXPERIMENTAL_COREPACK=1

# Prisma
# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema
# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings
DATABASE_URL='postgresql://myuser:mysecret@127.0.0.1:5432/mydb?search_path=public&connect_timeout=10'

# Redis
UPSTASH_REDIS_REST_URL="http://localhost:6380"
UPSTASH_REDIS_REST_TOKEN="mytoken"
UPSTASH_DISABLE_TELEMETRY=1 

# Better Auth Configuration
# Generate with: npx @better-auth/cli@latest secret
BETTER_AUTH_SECRET=mysecret

# Google OAuth Configuration
# Get these from: https://console.cloud.google.com/
GOOGLE_CLIENT_ID="myclientid"
GOOGLE_CLIENT_SECRET="mysecret"

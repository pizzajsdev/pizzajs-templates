{
  "name": "template-enterprise",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "pnpm codegen",
    "build": "react-router build",
    "build:prod": "pnpm build && pnpm db:migrate-prod",
    "postbuild:prod": "bun run --shell system scripts/prisma-engine-copy.ts",
    "build:pwa-assets": "./scripts/build-pwa-assets.sh",
    "codegen": "pnpm db:generate && react-router typegen",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate-prod": "prisma migrate deploy",
    "dev": "react-router dev --port 3000 --host 0.0.0.0 --open",
    "dev:debug": "concurrently \"pnpm dev:debug-routes\" \"pnpm dev\" \"react-scan http://localhost:3000\"",
    "dev:debug-routes": "react-router routes",
    "format": "sort-package-json && bun run --bun prettier --write .",
    "format:lint": "bun run --bun prettier --check .",
    "lint": "pnpm typecheck && pnpm format:lint && pnpm lint:madge && pnpm lint:eslint",
    "lint:eslint": "bun run --bun eslint app/ scripts/",
    "lint:madge": "bun run --bun madge --circular --extensions ts,tsx app/ scripts/ --exclude app/generated/",
    "prepare": "sort-package-json",
    "start": "PORT=4000 dotenv -e .env -v IS_DEV_BUILD=1 -v APP_URL=http://localhost:4000 react-router-serve ./build/server/index.js",
    "test": "bun test app/",
    "test:e2e": "playwright test tests-e2e/",
    "typecheck": "pnpm codegen && tsc",
    "update-deps": "pnpm -r up --latest --save",
    "postupdate-deps": "pnpm install && echo 'Installed dependencies again to apply overrides.'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@pizzajsdev/app-router-fs": "^0.3.2",
    "@prisma/client": "6.13.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@tailwindcss/typography": "^0.5.16",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "better-auth": "^1.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "is-ip": "^5.0.1",
    "isbot": "^5.1.29",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.536.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-intersection-observer": "^9.16.0",
    "react-router": "^7.7.1",
    "react-scan": "^0.4.3",
    "recharts": "^3.1.0",
    "remark-frontmatter": "^5.0.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "use-debounce": "^10.0.5",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.2",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/bun": "^1.2.19",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/web-app-manifest": "^1.0.9",
    "@vercel/react-router": "^1.2.2",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "concurrently": "^9.2.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc1",
    "madge": "^8.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.13.0",
    "sort-package-json": "^3.4.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vite-plugin-babel": "^1.3.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22.15.0"
  }
}
